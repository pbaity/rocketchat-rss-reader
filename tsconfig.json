{
    "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",
      "moduleResolution": "node",
      "declaration": false,
      "noImplicitAny": false,
      "removeComments": true,
      "strictNullChecks": true,
      "noImplicitReturns": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": ["es2020.string"],
    },
    "include": [
      "**/*.ts"
    ]
  }
